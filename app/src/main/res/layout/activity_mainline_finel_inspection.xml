<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f8931f"
    tools:context="com.example.a2mee.myrenata.com.net.activety.MSendtoFG">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="2dp"
        android:layout_marginRight="8dp"
        android:background="#fff">


        <ImageView
            android:id="@+id/sentfslogo"
            android:layout_width="120dp"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:background="@drawable/boxx"
            android:foregroundGravity="center" />

        <TextView
            android:id="@+id/finelqc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sentfslogo"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="50dp"
            android:text="Receive At Store"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="25sp" />

        <Button
            android:id="@+id/butscanfinelqc"
            android:layout_width="260dp"
            android:layout_height="50sp"
            android:layout_below="@+id/finelqc"
            android:layout_alignEnd="@+id/finelqc"
            android:layout_alignRight="@+id/finelqc"
            android:background="#0f7eb6"
            android:text="Scan"
            android:textColor="#a4f9f5f5"
            android:textSize="30sp" />

        <TextView
            android:id="@+id/barcodedataafqc"
            android:layout_width="match_parent"
            android:layout_height="40sp"
            android:layout_below="@+id/butscanfinelqc"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="7dp"
            android:layout_marginRight="30dp"
            android:hint=" Barcode data"
            android:textAlignment="center"
            android:textColor="#b1201f1f"
            android:textColorHint="#a4252424"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/qtyofstore"
            android:layout_width="100dp"
            android:layout_height="40sp"
            android:layout_below="@+id/barcodedataafqc"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="10dp"
            android:text="Quantity"
            android:textAlignment="center" />


        <EditText
            android:id="@+id/qtyatstoesed"
            android:layout_width="100dp"
            android:layout_height="40sp"
            android:layout_below="@+id/barcodedataafqc"
            android:layout_marginLeft="180dp"
            android:layout_marginTop="10dp"
            android:inputType="phone"
            android:textColor="#1d1c1c"
            android:textSize="20dp" />


        <TextView
            android:id="@+id/qoesed"
            android:layout_width="56dp"
            android:layout_height="40sp"
            android:layout_below="@+id/barcodedataafqc"
            android:layout_marginLeft="280dp"
            android:layout_marginTop="10dp"
            android:textAlignment="center"
            android:textColor="#1d1c1c"
            android:textSize="20dp" />

        <Button

            android:id="@+id/Submithhhfqc"
            android:layout_width="150dp"
            android:layout_height="50sp"
            android:layout_below="@+id/qtyofstore"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:background="#3ab54a"
            android:text="Submit"
            android:textSize="20dp" />


    </RelativeLayout>


</LinearLayout>
