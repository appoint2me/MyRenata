<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f8931f"
    tools:context="com.example.a2mee.myrenata.com.net.activety.Finel_material_inspActivity">

    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginEnd="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"
        android:layout_marginTop="4dp"
        android:background="#FFFF">

        <TextView
            android:id="@+id/nameonlineinsp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/relativeLayout"
            android:layout_alignParentTop="true"
            android:layout_alignRight="@+id/relativeLayout"
            android:gravity="center"
            android:text="Incoming Material Inspection"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="20dp" />

        <Button
            android:id="@+id/butscanfinel"
            android:layout_width="260dp"
            android:layout_height="50sp"
            android:layout_below="@+id/nameonlineinsp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="12dp"
            android:background="#007fbb "
            android:text="Scan"
            android:textColor="#a4f9f5f5"
            android:textSize="30sp" />


        <TextView
            android:id="@+id/barcodedataaf"
            android:layout_width="match_parent"
            android:layout_height="40sp"
            android:layout_below="@+id/butscanfinel"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:hint="Barcode data"
            android:textColor="#000"
            android:textColorHint="#000"
            android:textSize="15dp" />

      <!--  <TextView

            android:id="@+id/bagg"
            android:layout_width="250dp"
            android:layout_height="40sp"
            android:layout_below="@+id/barcodedataaf"
            android:layout_marginLeft="48dp"
            android:layout_marginRight="8dp"
            android:text="No of Bag's/Cartons"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="15dp" />

        <CheckBox
            android:id="@+id/bagg1"
            android:layout_width="50dp"
            android:layout_height="40sp"
            android:layout_alignEnd="@+id/barcodedataaf"
            android:layout_alignRight="@+id/barcodedataaf"
            android:layout_below="@+id/barcodedataaf"
            android:layout_marginEnd="12dp"
            android:layout_marginRight="12dp"
            android:textColor="#000"
            android:textSize="16dp" />-->

        <TextView

            android:id="@+id/lotqtty"
            android:layout_width="150dp"
            android:layout_height="40sp"
            android:layout_below="@+id/barcodedataaf"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"
            android:layout_marginTop="5dp"
            android:background="#544741"
            android:text="LotQty"
            android:textAlignment="center"
            android:textColor="#fff"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/lotqttyj"
            android:layout_width="150dp"
            android:layout_height="40sp"
            android:layout_below="@+id/barcodedataaf"
            android:layout_marginLeft="190dp"
            android:layout_marginTop="5dp"
            android:background="#a2f4e7e7"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="16dp" />

        <RelativeLayout
            android:id="@+id/relativeLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lotqtty"
            android:layout_marginLeft="1dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="5dp">

            <TextView
                android:id="@+id/accepiid1f"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_alignLeft="@+id/holdres1f"
                android:layout_alignParentTop="true"
                android:layout_alignStart="@+id/holdres1f"
                android:background="#007fbb "
                android:text="Accept"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="20dp" />


            <EditText
                android:imeOptions="actionNext"

                android:id="@+id/acceptQty"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="5dp"
                android:background="#a2f4e7e7"
                android:inputType="numberDecimal"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="16dp" />


            <TextView
                android:id="@+id/holdres1f"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/accepiid1f"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:layout_marginTop="5dp"
                android:background="#f3732c"
                android:text="Hold Reason"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="20dp" />

            <EditText
                android:imeOptions="actionNext"

                android:id="@+id/holdresf"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/acceptQty"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="2dp"
                android:background="#a2f4e7e7"
                android:inputType="text"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="16dp" />


            <TextView
                android:id="@+id/Hold1f"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/holdres1f"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"

                android:layout_marginTop="5dp"
                android:background="#f3732c"
                android:text="Hold Qty"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/Holdf"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/holdres1f"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="5dp"
                android:background="#a2f4e7e7"
                android:inputType="numberDecimal"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="16dp" />


            <TextView
                android:id="@+id/Rejection1f"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Hold1f"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:layout_marginTop="5dp"
                android:background="#666666"
                android:text="Rejection reason"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="18dp" />

            <Spinner
                android:id="@+id/Rejectionf"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Hold1f"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="5dp"
                android:background="#a2f4e7e7"
                android:textColor="#000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/Reject1f"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Rejection1f"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:layout_marginTop="5dp"
                android:background="#ee1c25"
                android:inputType="number"
                android:text="Rejected Qty"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="20dp" />

            <EditText


                android:id="@+id/Rejectf"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Rejection1f"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="5dp"
                android:background="#a2f4e7e7"
                android:inputType="numberDecimal"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/oprater1f"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Reject1f"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:layout_marginTop="5dp"
                android:background="#75a337"
                android:text="Operator ID"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/opraterf"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Reject1f"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="5dp"
                android:background="#a2f4e7e7"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="16dp" />

            <Button
                android:id="@+id/Submithhhf"
                android:layout_width="150dp"
                android:layout_height="50sp"
                android:layout_below="@+id/oprater1f"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="10dp"
                android:background="#3ab54a"
                android:text="Save"
                android:textColor="#fff"
                android:textSize="20dp" />

            <Button
                android:id="@+id/Submitkkf"
                android:layout_width="150dp"
                android:layout_height="50sp"
                android:layout_alignLeft="@+id/oprater1f"
                android:layout_alignStart="@+id/oprater1f"
                android:layout_below="@+id/oprater1f"
                android:layout_marginLeft="185dp"
                android:layout_marginStart="185dp"
                android:layout_marginTop="10dp"
                android:background="#007fbb "
                android:text="Save Another"
                android:textColor="#fff"
                android:textSize="20dp" />
        </RelativeLayout>
    </RelativeLayout>
</LinearLayout>
