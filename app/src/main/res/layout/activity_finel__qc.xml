<?xml version="1.0" encoding="utf-8"?>
<LinearLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a2mee.myrenata.com.net.activety.Finel_QC">


    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:background="#FFFF">

        <TextView
            android:id="@+id/nameonlineinspFC"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/relativeLayoutqq"
            android:layout_alignRight="@+id/relativeLayoutqq"
            android:layout_alignParentTop="true"
            android:gravity="center"
            android:text="Final Inspection"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="20dp" />

        <Button
            android:id="@+id/butscanfinelFC"
            android:layout_width="260dp"
            android:layout_height="50sp"
            android:layout_below="@+id/nameonlineinspFC"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="12dp"
            android:background="#007fbb "
            android:text="Scan"
            android:textColor="#a4f9f5f5"
            android:textSize="30sp" />


        <TextView
            android:id="@+id/barcodedataafFC"
            android:layout_width="match_parent"
            android:layout_height="40sp"
            android:layout_below="@+id/butscanfinelFC"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="30dp"
            android:gravity="center"
            android:hint="Barcode data"
            android:textColor="#000"
            android:textColorHint="#000"
            android:textSize="15dp" />

        <!--  <TextView

              android:id="@+id/baggFC"
              android:layout_width="250dp"
              android:layout_height="40sp"
              android:layout_below="@+id/barcodedataafFC"
              android:layout_marginLeft="48dp"
              android:layout_marginRight="8dp"
              android:text="No of Bag's/Cartons"
              android:textAlignment="center"
              android:textColor="#000"
              android:textSize="15dp" />

          <CheckBox
              android:id="@+id/bagg1FC"
              android:layout_width="50dp"
              android:layout_height="40sp"
              android:layout_alignEnd="@+id/barcodedataafFC"
              android:layout_alignRight="@+id/barcodedataafFC"
              android:layout_below="@+id/barcodedataafFC"
              android:layout_marginEnd="12dp"
              android:layout_marginRight="12dp"
              android:textColor="#000"
              android:textSize="16dp" />-->

        <TextView
            android:id="@+id/lotqttyFC"
            android:layout_width="150dp"
            android:layout_height="40sp"
            android:layout_below="@+id/barcodedataafFC"
            android:layout_marginLeft="18dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="18dp"
            android:background="#544741"
            android:text="LotQty"
            android:textAlignment="center"
            android:textColor="#fff"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/lotqttyjFC"
            android:layout_width="150dp"
            android:layout_height="40sp"
            android:layout_below="@+id/barcodedataafFC"
            android:layout_marginLeft="190dp"
            android:layout_marginTop="5dp"
            android:background="#a2f4e7e7"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="16dp" />

        <RelativeLayout
            android:id="@+id/relativeLayoutFC"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lotqttyFC"
            android:layout_marginLeft="1dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="8dp">

            <TextView
                android:id="@+id/accepiid1fFC"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_alignStart="@+id/holdres1fFC"
                android:layout_alignLeft="@+id/holdres1fFC"
                android:layout_alignParentTop="true"
                android:background="#007fbb "
                android:text="Accept"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="20dp" />


            <EditText
                android:id="@+id/acceptQtyFC"

                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="5dp"
                android:background="#a2f4e7e7"
                android:imeOptions="actionNext"
                android:inputType="numberDecimal"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="16dp" />


            <TextView
                android:id="@+id/holdres1fFC"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/accepiid1fFC"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="18dp"
                android:background="#f3732c"
                android:text="Hold Reason"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/holdresfFC"

                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/acceptQtyFC"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="2dp"
                android:background="#a2f4e7e7"
                android:imeOptions="actionNext"
                android:inputType="text"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="16dp" />


            <TextView
                android:id="@+id/Hold1fFC"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/holdres1fFC"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="18dp"
                android:background="#f3732c"
                android:text="Hold Qty"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/HoldfFC"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/holdres1fFC"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="5dp"
                android:background="#a2f4e7e7"
                android:inputType="numberDecimal"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="16dp" />


            <TextView
                android:id="@+id/Rejection1fFC"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Hold1fFC"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="18dp"
                android:background="#666666"
                android:text="Rejection reason"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="18dp" />

            <Spinner
                android:id="@+id/RejectionfFC"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Hold1fFC"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="5dp"
                android:background="#a2f4e7e7"
                android:textColor="#000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/Reject1fFC"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Rejection1fFC"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="18dp"
                android:background="#ee1c25"
                android:inputType="number"
                android:text="Rejected Qty"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="20dp" />

            <EditText

                android:id="@+id/RejectfFC"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Rejection1fFC"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="5dp"
                android:background="#a2f4e7e7"
                android:inputType="numberDecimal"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/oprater1fFC"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Reject1fFC"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="18dp"
                android:background="#75a337"
                android:text="operator ID"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/opraterfFC"
                android:layout_width="150dp"
                android:layout_height="40sp"
                android:layout_below="@+id/Reject1fFC"
                android:layout_marginLeft="190dp"
                android:layout_marginTop="5dp"
                android:background="#a2f4e7e7"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="16dp" />

            <Button
                android:id="@+id/SubmithhhfFC"
                android:layout_width="150dp"
                android:layout_height="50sp"
                android:layout_below="@+id/oprater1fFC"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="10dp"
                android:background="#3ab54a"
                android:text="Save"
                android:textColor="#fff"
                android:textSize="20dp" />

            <Button
                android:id="@+id/SubmitkkfFC"
                android:layout_width="150dp"
                android:layout_height="50sp"
                android:layout_below="@+id/oprater1fFC"
                android:layout_marginStart="185dp"
                android:layout_marginLeft="185dp"
                android:layout_marginTop="10dp"
                android:background="#007fbb "
                android:text="Save Another"
                android:textColor="#fff"
                android:textSize="18dp" />
        </RelativeLayout>
    </RelativeLayout>


</LinearLayout>
